generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id                String   @id @default(cuid())
  phone             String   @unique
  passwordHash      String
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
  dailyCalorieLimit Int @default(2400)
}
